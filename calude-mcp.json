{
    "mcpServers": {
        "supabase": {
            "command": "npx",
            "args": [
                "-y",
                "@supabase/mcp-server-supabase@latest",
                "--access-token",
                ""
            ]
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ]
        },
        "linear": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.linear.app/sse"
            ]
        },
        "macos_automator": {
            "command": "npx",
            "args": [
                "-y",
                "@steipete/macos-automator-mcp@latest"
            ]
        },
        "canvas-mcp-server": {
            "command": "npx",
            "args": ["-y", "canvas-mcp-server"],
            "env": {
                "CANVAS_API_TOKEN": "apikeyhere",
                "CANVAS_DOMAIN": "byui.instructure.com"
            }
        }

    },
    "globalShortcut": "Ctrl+Space"
}
